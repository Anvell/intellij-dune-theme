        "activeBackground": "$panelBackground",
        "inactiveBackground": "$panelBackground"
      },
      "Toolbar": {
        "background": "$panelBackground",
        "borderColor": "$toolwindowBackground"
      },
      "Advertiser": {
        "background": "$panelBackground",
        "borderColor": "$panelBackground",
        "foreground": "$mutedForeground"
      }
    },
    "PopupMenu": {
      "borderWidth": 1,
      "borderInsets": "4,1,4,1",
      "background": "$panelBackground",
      "translucentBackground": "$panelBackground",
      "foreground": "$regularForeground"
    },
    "ProgressBar": {
      "trackColor": "#FFFFFF",
      "progressColor": "#5a5a66B0",
      "indeterminateStartColor": "#5a5a66B0",
      "indeterminateEndColor": "#5a5a6640",
      "failedColor": "#d96462B0",
      "failedEndColor": "#d9646240",
      "passedColor": "#63994dB0",
      "passedEndColor": "#63994d40",
      "selectionForeground": "$regularForeground",
      "selectionBackground": "$regularForeground"
    },
    "RadioButton": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "disabledText": "$disabledForeground"
    },
    "RadioButtonMenuItem": {
      "background": "$panelBackground",
      "foreground": "$regularForeground",
      "selectionBackground": "$selectionBackground",
      "selectionForeground": "$selectionForeground",
      "acceleratorForeground": "$mutedForeground",
      "acceleratorSelectionForeground": "$selectionForeground",
      "disabledForeground": "$disabledForeground"
    },
    "ScrollBar": {
      "background": "$panelBackground",
      "trackColor": "$panelBackground",
      "Mac.trackColor": "$panelBackground",
      "hoverTrackColor": "$panelBackground",
      "Mac.hoverTrackColor": "$panelBackground",
      "Transparent.trackColor": "#E8E8E800",
      "Mac.Transparent.trackColor": "#E8E8E800",
      "Transparent.hoverTrackColor": "#E8E8E800",
      "Mac.Transparent.hoverTrackColor": "#E8E8E800",
      "thumbBorderColor": "#88889940",
      "Mac.thumbBorderColor": "#88889940",
      "hoverThumbBorderColor": "#888899",
      "Mac.hoverThumbBorderColor": "#888899",
      "thumbColor": "#88889940",
      "Mac.thumbColor": "#88889940",
      "hoverThumbColor": "#888899",
      "Mac.hoverThumbColor": "#888899",
      "Transparent.thumbBorderColor": "#88889940",
      "Mac.Transparent.thumbBorderColor": "#88889940",
      "Transparent.hoverThumbBorderColor": "#888899",
      "Mac.Transparent.hoverThumbBorderColor": "#888899",
      "Transparent.thumbColor": "#88889940",
      "Mac.Transparent.thumbColor": "#88889940",
      "Transparent.hoverThumbColor": "#888899",
      "Mac.Transparent.hoverThumbColor": "#888899"
    },
    "ScrollPane": {
      "background": "$panelBackground",
      "foreground": "$regularForeground"
    },
    "SearchEverywhere": {
      "Header.background": "$panelBackground",
      "Tab": {
        "selectedBackground": "$tabBackgroundSelected",
        "selectedForeground": "$selectionForeground"
      },
      "SearchField": {
        "background": "$textInputBackground",
        "borderColor": "$panelBackground",
        "infoForeground": "$disabledForeground"
