    "$mutedForeground": "#767680",
    "$disabledForeground": "#a7a7af",
    "$textInputBackground": "#FFFFFF",
    "$textInputBackgroundDisabled": "#F0F0F0",
    "$tabBackgroundSelected": "#FFFFFF",
    "$linkRegular": "#4E83BF",
    "$linkAccented": "#1270DB",
    "$buttonFace": "#F8F8F8",
    "$notificationBackground": "#E6EFFF",
    "$notificationBorder": "#D1D8E5",
    "$notificationForeground": "#2C3F61",
    "$validationErrorBackground": "#FFE3E3",
    "$validationErrorBorder": "#F3C9CA",
    "$validationErrorForeground": "#B5494F",
    "$validationErrorFocus": "#DB5860A0",
    "$validationErrorFocusInactive": "#DB586060",
    "$validationWarningBackground": "#FAEED5",
    "$validationWarningBorder": "#ECD9B1",
    "$validationWarningForeground": "#A87300",
    "$validationWarningFocus": "#DE971FA0",
    "$validationWarningFocusInactive": "#DE971F60",
    "$validationInformativeBackground": "#DCF0E1",
    "$validationInformativeBorder": "#C0DFC8",
    "$validationInformativeForeground": "#378C49",
    "$speedSearchBackground": "#F7E8A8",
    "$speedSearchMatchBackground": "#FAE080",
    "$searchMatchForeground": "#9C7A00",
    "$iconGrayRegular": "#82828F",
    "$iconGrayMuted": "#82828FB2",
    "$iconGrayDisabled": "#82828f66"
