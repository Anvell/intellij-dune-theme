  "colors": {
    "$titleBackground": "#EBEBEB",
    "$selectionBackground": "#A6CFFF",
    "$selectionBackgroundMuted": "#CDE4FC",
    "$selectionUnderline": "#99C8FF",
    "$selectionBackgroundInactive": "#D5D6D9",
    "$selectionUnderlineInactive": "#CBCDCF",
    "$selectionForeground": "#383848",
    "$selectionForegroundInactive": "#383848",
    "$selectionToolwindowHeader": "#DCE4F0",
    "$border": "#D8D8D8",
    "$controlBorder": "#D8D8D8",
    "$separator": "#D8D8D8",
    "$listSeparator": "#E5E5E5",
    "$focusedBorder": "#4982CC",
    "$controlFocus": "#A6CFFFC0",
    "$popupBorder": "#B0B0B0",
    "$toolwindowBackground": "#F8F8F8",
